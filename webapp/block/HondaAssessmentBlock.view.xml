<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout" controllerName="WarrantyClaim_MockUp.block.CustomerConcernBlockController">
	<sap.ui.layout.form:Form xmlns:sap.ui.layout.form="sap.ui.layout.form" editable="true" id="__form0" objectBindings="{WarrantyClaim>/}">
		<sap.ui.layout.form:layout>
	    	<sap.ui.layout.form:ResponsiveGridLayout/>
	    </sap.ui.layout.form:layout>	
		<sap.ui.layout.form:formContainers>
			<sap.ui.layout.form:FormContainer id="__container0" title="Honda Assessment Results" >
				<sap.ui.layout.form:formElements>
					<sap.ui.layout.form:FormElement id="__element0" >
						<sap.ui.layout.form:fields>
							<l:Grid>
								<l:content>							
									<l:VerticalLayout width="100%">
										<l:layoutData>
											<l:GridData span="L6"/>
										</l:layoutData>
										<Label text="Assessment Comments"/>
										<TextArea xmlns="sap.m" id="assessmentComments" rows="10" cols="120" placeholder="" editable="false"/>
									</l:VerticalLayout>
									<l:VerticalLayout width="100%">
										<l:layoutData>
											<l:GridData span="L6"/>
										</l:layoutData>
										<Label text="Assessment Codes"/>
										<TextArea xmlns="sap.m" id="assessmentCodes" rows="10" cols="120" placeholder="" editable="false"/>
									</l:VerticalLayout>
								</l:content>	
							</l:Grid>
						</sap.ui.layout.form:fields>
					</sap.ui.layout.form:FormElement>
				</sap.ui.layout.form:formElements>
			</sap.ui.layout.form:FormContainer>
		</sap.ui.layout.form:formContainers>
	</sap.ui.layout.form:Form>
</mvc:View>