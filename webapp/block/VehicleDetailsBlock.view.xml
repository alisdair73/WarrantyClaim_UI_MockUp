<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout">
	<sap.ui.layout.form:Form xmlns:sap.ui.layout.form="sap.ui.layout.form" editable="true" id="__form0" objectBindings="{WarrantyClaim>/}">
		<sap.ui.layout.form:layout>
	    	<sap.ui.layout.form:ResponsiveGridLayout/>
	    </sap.ui.layout.form:layout>
		<sap.ui.layout.form:formContainers>
			<sap.ui.layout.form:FormContainer id="__container0" title="Vehicle Details">
				<sap.ui.layout.form:formElements>
					<sap.ui.layout.form:FormElement id="__element0">
						<sap.ui.layout.form:fields>
							<l:Grid defaultSpan="L4 M6 S6">
								<l:content>
								<l:VerticalLayout width="100%">
									<Label text="Dealer Contact" labelFor="__input0"/>
									<Input id="dealerContact" value="{WarrantyClaim>DealerContact}"/>
								</l:VerticalLayout>
								<l:VerticalLayout width="100%">
									<Label text="Engine Number"/>
									<Input  id="engineNumber" value="{WarrantyClaim>EngineNumber}"/>
								</l:VerticalLayout>	
								<l:VerticalLayout width="100%">
									<Label text="PWA Number"/>
									<Input  id="PWANumber" value="{WarrantyClaim>AuthorisationNumber}"/>
								</l:VerticalLayout>
								<l:VerticalLayout width="100%">
									<Label text="VIN"/>
									<Input  id="VIN" value="{WarrantyClaim>VIN}" editable="{= !${WarrantyClaim>ClaimNumber}}"/>
								</l:VerticalLayout>					
								<l:VerticalLayout width="100%">
									<Label text="Recall Number"/>
									<Input  id="recallNumber" value="{WarrantyClaim>RecallNumber}"/>
								</l:VerticalLayout>	
								</l:content>
							</l:Grid>
						</sap.ui.layout.form:fields>
					</sap.ui.layout.form:FormElement>
				</sap.ui.layout.form:formElements>
			</sap.ui.layout.form:FormContainer>
		</sap.ui.layout.form:formContainers>
	</sap.ui.layout.form:Form>
</mvc:View>