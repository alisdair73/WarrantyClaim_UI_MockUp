{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "WarrantyClaim_MockUp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "Warranty Claim",
		"description": "Warranty Claim Maintenance",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "com.sap.webide.annotationmodeller.CreateAnnotationTemplate",
			"version": "1.0.0"
		},
		"dataSources": {
			"ZWTY_WARRANTY_CLAIMS_SRV": {
				"uri": "/sap/opu/odata/sap/ZWTY_WARRANTY_CLAIMS_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZWTY_WARRANTY_CLAIMS_SRV/metadata.xml",
					"annotations": [
						"annotation0"
					]
				}
			},
			"annotation0": {
				"uri": "annotation0.xml",
				"type": "ODataAnnotation",
				"settings": {
					"localUri": "annotation0.xml"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "WarrantyClaim_MockUp.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "WarrantyClaim_MockUp.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZWTY_WARRANTY_CLAIMS_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"viewPath": "WarrantyClaim_MockUp.view",
				"controlId": "app",
				"viewType": "XML",
				"controlAggregation": "pages",
				"viewLevel": 1
			},
			"routes": [
				{
					"name": "createWarranty",
					"pattern": "",
					"target": [
						"warrantyMain"
					]
				},
				{
					"name": "maintainWarranty",
					"pattern": "Claim/{warrantyClaimNumber}",
					"target": [
						"warrantyMain"
					]
				}
			],
			"targets": {
				"warrantyMain": {
					"viewName": "WarrantyClaimObjectPage",
					"viewId": "warrantyMain",
					"viewLevel": 1,
					"transition": "show"
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zwty_claim_demo/webapp",
		"_version": "1.1.0"
	}
}