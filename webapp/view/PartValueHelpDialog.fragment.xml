<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:vh="sap.ui.comp.valuehelpdialog">
	<SelectDialog 
		noDataText="No Parts Found"	
		title="Select Part"
		class="sapUiPopupWithPadding"
		items="{/ZDPE_C_MATERIALS}"
		search="onValueHelpSearch"
		confirm="onPartSelected"
		cancel="handleValueHelpClose"
		multiSelect="false">
		<StandardListItem
			title="{materialNo}"
			description="{description}" />
	</SelectDialog>
</core:FragmentDefinition>