<mvc:View controllerName="WarrantyClaim_MockUp.controller.SymptomCodeSelector" xmlns="sap.ui.table" xmlns:m="sap.m" xmlns:mvc="sap.ui.core.mvc">
	<TreeTable
		id="TreeTableBasic"
        rows="{path:'SymptomCodes>/SymptomCodes', parameters: {arrayNames:['children']}}"
        selectionMode="Single"
        enableSelectAll="false"
        ariaLabelledBy="title">
        <columns>
        	<Column>
            	<m:Label text="Select Symptom Codes"/>
                <template>
                	<m:Text text="{SymptomCodes>Name}"/>
                </template>
            </Column>
        </columns>
	</TreeTable>
</mvc:View>    